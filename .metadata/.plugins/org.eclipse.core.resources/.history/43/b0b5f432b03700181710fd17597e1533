package application;

public class Record {
	private Integer idHospede;
	private Integer channel;
	private String record;
	
	public Record(){
		this.idHospede = 0;
		this.channel = 0;
		this.record = "";
	}
	
	public Record(Integer idHospede, Integer channel, String record){
		this.idHospede = idHospede;
		this.channel = channel;
		this.record = record;
	}
	
	void setIdHospede(Integer idHospede) {
		this.idHospede = idHospede;
	}
	
	void setChannel(Integer channel) {
		this.channel = channel;
	}
	
	void setRecord(String record) {
		this.record = record;
	}
	
	Integer getIdHospede() {
		return this.idHospede;
	}
	
	Integer getChannel() {
		return this.channel;
	}
	
	String getRecord() {
		return this.record;
	}
}
